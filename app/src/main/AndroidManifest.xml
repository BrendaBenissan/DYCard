<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.simon.dycard">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_dycard"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ConnexionActivity" />
        <activity android:name=".InscriptionActivity" />
        <activity android:name=".FirstActivity" />
        <activity android:name=".activity_destinataire" />
        <activity android:name=".activity_texte" />
        <activity android:name=".Activity_mail" />
        <activity android:name=".ActivityValidation" />
        <activity android:name=".Validation"></activity>
    </application>

</manifest>